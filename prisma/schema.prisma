generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

model users {
  id        Int       @id @default(autoincrement())
  name      String
  email     String    
  password  String
  mobile    String
  status    String    @default("active")
  role      String    @default("user")
  image     String?
  dob       DateTime?
  createdAt DateTime  @default(now())
}
 npx prisma migrate dev --name init
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

model users {
  id        Int      @id @default(autoincrement())
  name      String
  password  String
  mobile    String?
  status    String   @default("active")
  role      String   @default("user")
  image     String?
  dob       DateTime?
  createdAt DateTime @default(now())
  email     String   @unique
}

